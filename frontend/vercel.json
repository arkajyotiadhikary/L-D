{
      "routes": [
            { "src": "/(.*)", "dest": "/" },
            {
                  "src": "/admin/:path*",
                  "dest": "/"
            },
            {
                  "src": "/dashboard",
                  "dest": "/"
            },
            {
                  "src": "/module/:id",
                  "dest": "/"
            },
            {
                  "src": "/module/:module/content/:content",
                  "dest": "/"
            },
            {
                  "src": "/module/:module/assignment/:assignment",
                  "dest": "/"
            }
      ]
}
